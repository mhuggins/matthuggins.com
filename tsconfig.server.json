    "outDir": "./dist",
    "rootDir": "./src",
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "include": ["src/server.ts"]
}
